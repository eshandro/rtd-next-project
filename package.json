{
  "name": "rtd-project",
  "version": "0.0.1",
  "description": "RTD Next Ride finder",
  "private": true,
  "repository": "https://github.com/eshandro/rtd-next-project.git",
  "author": "eshandro <erik843@gmail.com>",
  "license": "MIT",
  "scripts": {
    "updateFeed": "node src/server/static_feed/initUpdateStaticFeed.js",
    "forceUpdateFeed": "node src/server/static_feed/initUpdateStaticFeed.js true",
    "test": "NODE_ENV=test nodemon --exec 'mocha --recursive'",
    "production": "NODE_ENV=production node src/client/index.js",
    "lint": "./node_modules/.bin/eslint src",
    "client": "webpack-dev-server --open --colors --mode development",
    "server": "nodemon --exec node src/server/server.js",
    "start": "concurrently --kill-others-on-fail \"npm:server\" \"npm:client\"",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.0.1",
    "eslint": "^5.6.1",
    "eslint-plugin-react": "^7.9.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^5.0.1",
    "nodemon": "^1.15.1",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "fs-readfile-promise": "^3.0.0",
    "mongoose": "^5.3.2",
    "node-fetch": "^2.1.2",
    "path-reader": "^1.0.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "stream-json": "^1.1.3",
    "unzipper": "^0.9.4"
  }
}
